#include <stdio.h>

int main() {
    int vetor[5];
    int i;

    printf("Digite 5 números:\n");

    // Lendo os números e armazenando no vetor
    for(i = 0; i < 5; i++) {
        printf("Número %d: ", i + 1);
        scanf("%d", &vetor[i]);
    }

    // Exibindo os números na tela
    printf("\nOs números digitados são:\n");
    for(i = 0; i < 5; i++) {
        printf("%d ", vetor[i]);
    }

    return 0;
}
